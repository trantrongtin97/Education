@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewData["Title"]</title>
</head>
<body>
    <h1>@ViewData["Header"]</h1>
    @{
        var student = ViewData["Student"] as AspNetCore_WebApp_Mvc_ViewData_ViewBag_StronglyTyped.Models.Student;
    }
    <div>
        StudentId : @student?.Id
    </div>
    <div>
        Name : @student?.Name
    </div>
</body>
</html>